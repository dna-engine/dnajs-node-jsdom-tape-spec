{
   "name": "node-jsdom-tape-spec",
   "version": "1.0.0",
   "description": "Sample dna-engine project running on Node.js and jsdom executing tape test cases",
   "license": "MIT",
   "type": "module",
   "module": "app.js",
   "repository": "github:dna-engine/node-jsdom-tape-spec",
   "homepage": "https://github.com/dna-engine/node-jsdom-tape-spec",
   "bugs": "https://github.com/dna-engine/node-jsdom-tape-spec/issues",
   "docs": "https://dna-engine.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "dna-engine",
      "node",
      "jsdom",
      "tape",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true
   },
   "scripts": {
      "pretest": "html-validator && jshint . --exclude node_modules",
      "test": "tape spec/*.spec.js"
   },
   "dependencies": {
      "dna-engine": "~2.3",
      "jquery": "~3.6"
   },
   "devDependencies": {
      "canvas": "~2.11",
      "jsdom": "~22.0",
      "jshint": "~2.13",
      "tap-spec": "~5.0",
      "tape": "~5.6",
      "w3c-html-validator": "~1.3"
   }
}
