{
   "name":        "dnajs-node-jsdom-tape-spec",
   "version":     "1.0.0",
   "description": "Sample dna.js project running on Node.js and jsdom executing tape test cases",
   "license":     "MIT",
   "main":        "app.js",
   "repository":  "dnajs/dnajs-node-jsdom-tape-spec.git",
   "homepage":    "https://github.com/dnajs/dnajs-node-jsdom-tape-spec",
   "bugs":        "https://github.com/dnajs/dnajs-node-jsdom-tape-spec/issues",
   "docs":        "http://dnajs.org/docs",
   "keywords":    ["dna.js", "node", "jsdom", "tape", "javascript"],
   "author":      { "name": "Center Key", "url": "http://centerkey.com" },
   "jshintConfig": {
      "esversion": 6,
      "strict":    "implied",
      "node":      true,
      "undef":     true,
      "unused":    true
   },
   "scripts": {
      "pretest": "jshint *.js",
      "test":    "tape spec.js"
   },
   "dependencies": {
      "dna.js": "~1.2"
   },
   "devDependencies": {
      "jsdom":    "~11.0",
      "jshint":   "~2.9",
      "tap-spec": "~4.1",
      "tape":     "~4.6"
   }
}
